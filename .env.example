# Hugging Face token (required to download gated models)
HF_TOKEN=hf_your_token_here

# Model configuration
MODEL_NAME=Qwen/Qwen2.5-Omni-7B
MODEL_PATH=/runpod-volume/models/Qwen2.5-Omni-7B
DTYPE=bfloat16

# vLLM serving
PORT=8000
HOST=0.0.0.0
GPU_MEMORY_UTILIZATION=0.90
MAX_MODEL_LEN=4096

# RunPod network volume mount point
VOLUME_PATH=/runpod-volume
